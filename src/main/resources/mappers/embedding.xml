<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.enjoytrip.domain.trip.mapper.EmbeddingMapper">
    <select id="findByName" parameterType="String" resultType="EmbeddingDto">
        select *
        from attraction_embedding
        where embedding_name=#{name}
    </select>

</mapper>